    let lista1 = document.createElement("li");
    lista1.className = "lista-de-tarefas";
    document.getElementById("lista-de-elementos-adicionados").appendChild(lista1)
    tag.innerHTML += lista1;
    lista1.value = '';
    lista1.focus();
})



